{
  "name": "hu-solver-dev",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "postCreateCommand": "python -m pip install --upgrade pip && pip install maturin numpy jsonschema pyyaml",
  "customizations": {
    "vscode": {
      "settings": {
        "python.defaultInterpreterPath": "/usr/local/bin/python"
      },
      "extensions": [
        "ms-python.python",
        "rust-lang.rust-analyzer",
        "ms-toolsai.jupyter"
      ]
    }
  },
  "remoteUser": "vscode"
}
